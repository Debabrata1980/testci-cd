AWSTemplateFormatVersion: '2010-09-09'
Description: sftp user creation

Resources:
  UserTest:
    Type: AWS::Transfer::User
    Properties:
      ServerId: s-8e3d46b512f94e12b
      UserName: testuser1
      HomeDirectoryMappings:
      - Entry: "/"
        Target: "/buck5tr/test"
      HomeDirectoryType: LOGICAL
      Role: "arn:aws:iam::273641968769:role/buck5role"
      SshPublicKeys:
        - ssh-rsa  rsa-key-20210911
Outputs:
    Username:
      Value: !Ref UserTest
      Description: Name of user



